    "@capacitor/keyboard": "^5.0.7",
    "@capacitor/status-bar": "^5.0.6",
    "@tanstack/react-query": "^5.17.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "zustand": "^4.5.0",
    "recharts": "^2.10.4",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@types/react": "^18.2.48",
